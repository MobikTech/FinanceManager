@model IEnumerable<AccountViewModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Available accounts</title>
</head>
<body>
    <div>
        <h3>Accounts</h3>
        <table>
            <tr>
                <td>Number</td>
                <td>Count</td>
                @* <td></td> *@
            </tr>
            @foreach (var account in Model)
            {
                <tr>
                    <td>@account.Number</td>
                    <td>@account.Count</td>
                    @* <td> *@
                    @*     <a href="~/Home/DeleteAccount/@account.Number">Delete</a> *@
                    @* </td> *@
                </tr>
            }
        </table>
    </div>
</body>
</html>